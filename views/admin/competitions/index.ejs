<% include ../../partials/admin %>
<div class="container">
    <a class='btn btn-success' href='/admin/zavody/nove'>Vytvořit nový závod</a>
    <div class="row text-center" style="display: flex; flex-wrap: wrap;">
        <% competitions.forEach(function(competition) { %>
            <div class="col-md-4 col-sm-6">
                <div class="thumbnail">
                    <img src="<%= competition.map %>">
                    <div class="caption">
                        <h4><%= competition.title %></h4>
                    </div>
                    <p>
                        <a class="btn btn-primary" href="/admin/zavody/<%= competition._id %>">Vice</a>
                    </p>
                </div>
            </div>
        <% }); %>
    </div>
</div>

</body>

</html>